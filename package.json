{
  "name": "create-stack-craft",
  "version": "1.1.4",
  "description": "A CLI tool to create projects with Express, Hono, and Prisma",
  "bin": {
    "create-stack-craft": "dist/cli.js"
  },
  "scripts": {
    "dev": "tsx src/cli.tsx",
    "build": "tsc",
    "start": "node dist/cli.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write 'src/**/*.{ts,js}' 'src/*.{ts,js,tsx}'"
  },
  "keywords": [
    "express",
    "hono",
    "prisma",
    "cli",
    "project generator",
    "template",
    "templates",
    "stack",
    "stack craft"
  ],
  "author": "Rio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pageton/create-stack-craft.git"
  },
  "bugs": {
    "url": "https://github.com/pageton/create-stack-craft/issues"
  },
  "homepage": "https://github.com/pageton/create-stack-craft#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0",
    "ink": "^5.0.1",
    "ink-select-input": "^6.0.0",
    "ink-text-input": "^6.0.0",
    "path": "^0.12.7",
    "prettier": "^3.3.3",
    "simple-git": "^3.27.0",
    "tsx": "^4.19.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.11",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^9.11.1",
    "typescript": "^5.6.2"
  },
  "files": [
    "dist",
    "templates",
    "LICENSE",
    "README.md"
  ],
  "type": "module"
}
